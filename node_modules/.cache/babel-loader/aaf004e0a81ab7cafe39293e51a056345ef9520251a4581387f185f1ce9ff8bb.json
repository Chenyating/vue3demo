{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { cubeCacheStore } from './store/index.js';\nexport default {\n  data() {\n    return {\n      cubeCache: cubeCacheStore(),\n      editableTabsValue: '0',\n      editableTabs: []\n    };\n  },\n  methods: {\n    goEdit(tab) {\n      if (this.editableTabsValue == tab.name) {\n        return;\n      }\n      this.$router.push({\n        path: `/edit?cacheKey=${tab.name}`\n      }); // 更新路由\n    },\n\n    handleTabsEdit(targetName, action) {\n      if (action === 'add') {\n        var cacheKey = this.cubeCache.getCacheKey();\n        this.editableTabs.push({\n          title: '编辑',\n          name: cacheKey,\n          content: '编辑',\n          cacheKey: cacheKey\n        });\n        this.editableTabsValue = cacheKey;\n        this.$router.push({\n          path: `/edit?cacheKey=${cacheKey}`\n        }); // 更新路由\n      }\n\n      if (action === 'remove') {\n        this.editableTabs = this.editableTabs.filter(item => item.name !== targetName);\n      }\n    }\n  }\n};","map":{"version":3,"names":["cubeCacheStore","data","cubeCache","editableTabsValue","editableTabs","methods","goEdit","tab","name","$router","push","path","handleTabsEdit","targetName","action","cacheKey","getCacheKey","title","content","filter","item"],"sources":["src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <br/>缓存数据：<br/>\n    <el-tag   v-for=\"(item, index) in editableTabs\" type=\"success\">{{item.name}}</el-tag>\n    <br/>缓存信息：<br/>\n\n    <br/>路由示例图：<br/>\n    <el-tabs\n      v-model=\"editableTabsValue\"\n      type=\"card\"\n      @tab-click=\"goEdit\"\n      editable\n      @edit=\"handleTabsEdit\"\n    >\n      <el-tab-pane\n        :key=\"item.name\"\n        v-for=\"(item, index) in editableTabs\"\n        :label=\"item.title + item.name\"\n        :name=\"item.name\"\n        :id=\"`tab-${item.cacheKey}`\"\n      >\n        <router-view />\n      </el-tab-pane>\n    </el-tabs>\n  </div>\n</template>\n\n<script>\nimport { cubeCacheStore } from './store/index.js'\n\nexport default {\n  data() {\n    return {\n      cubeCache: cubeCacheStore(),\n      editableTabsValue: '0',\n      editableTabs: [],\n    }\n  },\n  methods: {\n    goEdit(tab) {\n      if (this.editableTabsValue == tab.name) {\n        return\n      }\n      this.$router.push({ path: `/edit?cacheKey=${tab.name}` }) // 更新路由\n    },\n    handleTabsEdit(targetName, action) {\n      if (action === 'add') {\n        var cacheKey = this.cubeCache.getCacheKey()\n        this.editableTabs.push({\n          title: '编辑',\n          name: cacheKey,\n          content: '编辑',\n          cacheKey: cacheKey,\n        })\n        this.editableTabsValue = cacheKey\n        this.$router.push({ path: `/edit?cacheKey=${cacheKey}` }) // 更新路由\n      }\n      if (action === 'remove') {\n        this.editableTabs = this.editableTabs.filter(\n          (item) => item.name !== targetName\n        )\n      }\n    },\n  },\n}\n</script>\n"],"mappings":";AA4BA,SAAAA,cAAA;AAEA;EACAC,KAAA;IACA;MACAC,SAAA,EAAAF,cAAA;MACAG,iBAAA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAAC,GAAA;MACA,SAAAJ,iBAAA,IAAAI,GAAA,CAAAC,IAAA;QACA;MACA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA,oBAAAJ,GAAA,CAAAC,IAAA;MAAA;IACA;;IACAI,eAAAC,UAAA,EAAAC,MAAA;MACA,IAAAA,MAAA;QACA,IAAAC,QAAA,QAAAb,SAAA,CAAAc,WAAA;QACA,KAAAZ,YAAA,CAAAM,IAAA;UACAO,KAAA;UACAT,IAAA,EAAAO,QAAA;UACAG,OAAA;UACAH,QAAA,EAAAA;QACA;QACA,KAAAZ,iBAAA,GAAAY,QAAA;QACA,KAAAN,OAAA,CAAAC,IAAA;UAAAC,IAAA,oBAAAI,QAAA;QAAA;MACA;;MACA,IAAAD,MAAA;QACA,KAAAV,YAAA,QAAAA,YAAA,CAAAe,MAAA,CACAC,IAAA,IAAAA,IAAA,CAAAZ,IAAA,KAAAK,UACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}